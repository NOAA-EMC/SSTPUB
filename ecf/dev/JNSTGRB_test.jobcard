#!/bin/bash
##PBS -q dev_shared
#PBS -A GFS-DEV
#PBS -j oe
#PBS -N jnsst
#PBS -l select=1:ncpus=2:mem=2000MB
#PBS -l walltime=00:10:00

set -x

export envir=test         # prod, para or test
export job=jnsst
export pid=$$
export jobid=${job}.$pid
export RUN_ENVIR=test

. $MODULESHOME/init/ksh

module purge
module load envvar/1.0
module load PrgEnv-intel/8.1.0
module load craype/2.7.8
module load cray-mpich/8.1.7
module load intel/19.1.3.304
module load w3nco/2.4.1
module load bacio/2.4.1
module load nemsio/2.5.2
module load netcdf/4.7.4

module load prod_envir/2.0.5
module load prod_util/2.0.8
module load grib_util/1.2.2
module load libjpeg/9c
module load wgrib2/2.0.8
module load gempak/7.14.0
module load ecflow/5.6.0.6

module list

export cyc=12
export CDUMP=gdas

export DATAROOT=/gpfs/dell1/nco/ops/tmpnwprd
export DBNROOT=/iodprod_dell/dbnet_siphon

export jlogfile=${COMROOT}/logs/jlogfiles/jlogfile.${job}.${pid}


if [ "$envir" = 'test' ] ; then
  export NWROOT=/lfs/h2/emc/da/noscrub/Xu.Li
  export COMROOT_test=/lfs/h2/emc/da/noscrub/Xu.Li/com/gfs/${envir}
  export DATAROOT=/lfs/h2/emc/ptmp/Xu.Li/nsst/work
  export DBNROOT=${UTILROOT}/fakedbn

  mkdir -p $COMROOT_test
  mkdir -p $DATAROOT
# mkdir -p $DBNROOT

  export jlogfile=${COMROOT_test}/jlogfile.${job}.${pid}
  mkdir -p ${COMROOT_test}/logs
fi

export SENDECF=YES
export SENDDBN=YES
export SENDDBN_NTC=NO
export SENDCOM=YES
export SENDWEB=NO
export KEEPDATA=YES
export model=nsst

modelver=v1.2

export HOMEnsst=${NWROOT}/${model}.$modelver
ksh ${HOMEnsst}/ecf/dev/JNSST_test
